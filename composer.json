{
  "name": "wnmachado/brazilian_documents_tools",
  "description": "Classe em PHP para validação de CPF e CNPJ.",
  "keywords": [
    "cnpj",
    "cpf",
    "validador",
    "documento",
    "brasil",
    "laravel"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/wnmachado/brazilian_documents_tools",
  "authors": [
    {
      "name": "Wellington Machado",
      "email": "wellington.master@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Bissolli\\ValidadorCpfCnpj\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bissolli\\ValidadorCpfCnpj\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ]
  },
  "version": "1.0.0"
}
